{% extends "base.html.twig" %}

{% block body %}
<div class="notes" id="app">
    <Notelist 
      :notes="notes"
      v-on:select-note="changeNote($event)"
      v-on:new-note="newNote"
    >
    </Notelist>
    <Notepad :note="note"></Notepad>
    <Share :visible="1" :participants="participants"></Share>
</div>
{% endblock %}

{% block javascripts %}
  {{ encore_entry_script_tags('app') }}
{% endblock %}
